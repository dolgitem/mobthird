<?xml version='1.0' encoding='utf-8'?>
<widget id="listok.listokdelta" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ListokMobile</name>
    <description />
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="about:*" />
    <platform name="ios">
        <preference name="ScrollEnabled" value="true" />
    </platform>
    <platform name="android">
        <resource-file src="src-cordova/google-services.json" target="app/google-services.json" />
    </platform>
</widget>
